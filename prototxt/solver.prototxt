lr_policy: "poly"
power: 1.0
momentum: 0.9
weight_decay: 0.0005
snapshot: 150140
snapshot_prefix: "snapshot"
solver_mode: GPU
random_seed: 42
net: "train_val.prototxt"
solver_type: SGD
